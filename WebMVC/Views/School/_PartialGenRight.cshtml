<div class="panel panel-info">
    <div class="panel-heading">
        <div class="panel-title">Institution Info</div>
    </div>
    <div class="panel-body">
        <div class="col-md-0" style="text-align:left;">
            <p style="text-align:left">
                <span style="text-align:left; color: #FF0000;">NEMIS CODE: </span>@Session["Institution_Code"].ToString()
            </p>
            <p style="text-align:left">
                <span style="text-align:left; color: #FF0000;">School Level: </span>@Session["Institution_Level"].ToString()
            </p>
            <p style="text-align:left">
                <span style="text-align:left; color: #FF0000;">School Type: </span>@Session["Institution_Type"].ToString()
            </p>
        </div>
    </div>
</div>